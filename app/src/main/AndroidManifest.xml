<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ecd.redevi">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icono"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icono"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.ecd.redevi.Habitaciones"
            android:label="@string/title_activity_presupuestos"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name="com.ecd.redevi.Mueble"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.ecd.redevi.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>